{
  "name": "open-api-test",
  "version": "1.0.0",
  "description": "this is open-api-test",
  "main": "index.js",
  "repository": "https://github.com/UndyingSugimoto/openapi-test.gi",
  "author": "UndyingSugimoto <kouji36000@yahoo.co.jp>",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-cli": "^4.0.2",
    "@openapitools/openapi-generator-cli": "^1.0.13-4.3.1"
  },
  "scripts": {
    "bandle": "npx swagger-cli bundle ./reference/* -o ./bundled/output.yaml -r ./models/* -t yaml",
    "generate": "npx openapi-generator  generate -i ./bundled/output.yaml -g go-server -o ./generated",
    "build": "npm run bandle && npm run generate"
  }
}
